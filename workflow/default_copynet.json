{
  "dataset_reader": {
    "target_namespace": "target_tokens",
    "type": "copynet_coordinates",
    "source_token_indexers": {
      "tokens": {
        "type": "single_id",
        "namespace": "source_tokens"
      },
      "elmo": {
        "type": "elmo_characters"
      },
      "token_xbins":{
	"type": "coordinates_bin",
	"namespace": "token_xbins",
	"coordinate_index": 0,
	"nbins": 64
	},
      "token_ybins":{
	"type": "coordinates_bin",
	"namespace": "token_ybins",
	"coordinate_index": 1,
	"nbins": 128
	}
    },
     "source_tokenizer":{
	"type": "whitespace"
	}

  },
  "vocabulary": {
    "min_count": {
      "source_tokens": 1,
      "target_tokens": 1
    },
    "inclusion_rules":{
	"target_tokens": "@.*@"}
  },
  "train_data_path": null,
  "validation_data_path": null,
  "model": {
    "type": "copynet_seq2seq",
    "source_embedder": {
      "token_embedders": {
        "tokens": {
          "type": "empty"
        },
        
        "elmo":{
            "type": "elmo_token_embedder",
            "options_file": "https://s3-us-west-2.amazonaws.com/allennlp/models/elmo/2x1024_128_2048cnn_1xhighway/elmo_2x1024_128_2048cnn_1xhighway_options.json",
            "weight_file": "https://s3-us-west-2.amazonaws.com/allennlp/models/elmo/2x1024_128_2048cnn_1xhighway/elmo_2x1024_128_2048cnn_1xhighway_weights.hdf5",
            "do_layer_norm": false,
            "dropout": 0.1,
            "requires_grad": true

        },

        
	 "token_xbins": {
          "type": "embedding",
          "vocab_namespace": "token_xbins",
          "embedding_dim": 32,
          "trainable": true 
        },
	 "token_ybins": {
          "type": "embedding",
          "vocab_namespace": "token_ybins",
          "embedding_dim": 32,
          "trainable": true 
        }
    
      }
    },
    "encoder": {
      "type": "lstm",
      "input_size": 320,
      "hidden_size": 128,
      "num_layers": 1,
      "dropout": 0.1,
      "bidirectional": true
    },
    "attention": {
      "type": "bilinear",
      "vector_dim": 256,
      "matrix_dim": 256
    },
    "target_embedding_dim": 128,
    "beam_size": 1,
    "max_decoding_steps": 100,
    "restrict_to_single_copy": true,
    "add_source_embeddings_to_decoder": true,
   "tensor_based_metric": null,
   "token_based_metric": "information_extraction"
  },
  "iterator": {
    "type": "bucket",
    "padding_noise": 0.0,
    "batch_size": 1
  },
  "trainer": {
    "optimizer": {
      "type": "adam",
      "lr": 0.001
    },
    "num_epochs": 10,
    "cuda_device": 0,
    "grad_norm": 0.1,
    "validation_metric": "+f1",
    "should_log_learning_rate": true,
    "should_log_parameter_statistics": true
  }
}
